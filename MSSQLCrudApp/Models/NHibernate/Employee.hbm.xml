<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="MSSQLCrudApp" namespace="MSSQLCrudApp.Models">
  <class name="Employee" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="ID" type="int">
      <generator class="native" />
    </id>
    <property name="FullName" />
    <property name="Salary" />
    <property name="Speed" />
    <property name="Accuracy" />
    <property name="Efficiency" />
  </class>
</hibernate-mapping>